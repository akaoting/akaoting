[
  {
    "test": ".*\\.js$",
    "jsc": {
      "target": "es2022",
      "parser": {
        "syntax": "ecmascript"
      }
    }
  },
  {
    "test": ".*\\.jsx$",
    "jsc": {
      "baseUrl":"./",
      "paths": {
         "@/*": ["src/*"]
      },
      "target": "es2022",
      "parser": {
        "syntax": "ecmascript",
        "jsx": true
      },
      "transform": {
        "react": {
          "runtime": "automatic",
          "development": false,
          "refresh": false
        }
      }
    }
  }
]
